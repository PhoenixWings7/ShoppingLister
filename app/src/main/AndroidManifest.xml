<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.phoenixwings7.shoppinglisterassignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShoppingListerAssignment">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.NewListActivity"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".view.ListDetailsActivity"
            android:parentActivityName=".view.MainActivity"
            android:label="@string/list_details_activity_label"/>
    </application>

</manifest>